[core]
	excludesfile = ~/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
[include]
	path = .gitconfig.local
[alias]
	ad = add
	st = status
	co = commit
	ch = checkout
	df = diff
	dfc = diff --cached
	br = branch
	sp = stash pop
	ss = stash save
	gr = grep
	l = log --graph --decorate
	ps = push
	pl = pull
	see = browse
	sw = switch
